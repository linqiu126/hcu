		l3bfscResp.billtodayaccuvolume = rand()%1000000;
		l3bfscResp.billtodayaccuvolumeunit = HCU_NBIOT_CJ188_UNIT_M3;
		l3bfscResp.l3bfscData.lastmonth = 11;
	}
	else if ((rcv.l3bfscHead.ctrlId == HCU_NBIOT_CJ188_CTRL_READ_DATA) && (rcv.l3bfscHead.d0d1Id == HCU_NBIOT_CJ188_READ_DI0DI1_HISTORY_COUNTER_DATA12)){
