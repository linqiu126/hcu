	else if ((rcv.l3bfscHead.ctrlId == HCU_NBIOT_CJ188_CTRL_WRITE_DATA) && (rcv.l3bfscHead.d0d1Id == HCU_NBIOT_CJ188_WRITE_DI0DI1_BILL_TODAY_DATE)){
		//设置假数据，为了测试方便
		//Do nothing
	}
	else if ((rcv.l3bfscHead.ctrlId == HCU_NBIOT_CJ188_CTRL_WRITE_DATA) && (rcv.l3bfscHead.d0d1Id == HCU_NBIOT_CJ188_WRITE_DI0DI1_READ_ACCOUNT_CUR_DATE)){
		//设置假数据，为了测试方便
		//Do nothing
	}
	else if ((rcv.l3bfscHead.ctrlId == HCU_NBIOT_CJ188_CTRL_WRITE_DATA) && (rcv.l3bfscHead.d0d1Id == HCU_NBIOT_CJ188_WRITE_DI0DI1_BUY_AMOUNT)){
		//设置假数据，为了测试方便
		l3bfscResp.l3bfscData.buycode = rand()%256;
		l3bfscResp.l3bfscData.thisamount = rand()%1000000;
	}
	else if ((rcv.l3bfscHead.ctrlId == HCU_NBIOT_CJ188_CTRL_WRITE_DATA) && (rcv.l3bfscHead.d0d1Id == HCU_NBIOT_CJ188_WRITE_DI0DI1_NEW_KEY)){
		//设置假数据，为了测试方便
		l3bfscResp.l3bfscData.keyver = rand()%256;
	}
	else if ((rcv.l3bfscHead.ctrlId == HCU_NBIOT_CJ188_CTRL_WRITE_DATA) && (rcv.l3bfscHead.d0d1Id == HCU_NBIOT_CJ188_WRITE_DI0DI1_STD_TIME)){
		//设置假数据，为了测试方便
		//Do nothing
	}
	else if ((rcv.l3bfscHead.ctrlId == HCU_NBIOT_CJ188_CTRL_WRITE_DATA) && (rcv.l3bfscHead.d0d1Id == HCU_NBIOT_CJ188_WRITE_DI0DI1_SWITCH_CTRL)){
		//设置假数据，为了测试方便
		memset(tmp, 0, sizeof(tmp));
		sprintf(tmp, "%04d", rand()%10000);
		strncpy(l3bfscResp.l3bfscData.st, tmp, 4);
	}
	else if ((rcv.l3bfscHead.ctrlId == HCU_NBIOT_CJ188_CTRL_WRITE_DATA) && (rcv.l3bfscHead.d0d1Id == HCU_NBIOT_CJ188_WRITE_DI0DI1_OFF_FACTORY_START)){
		//设置假数据，为了测试方便
		//Do nothing
	}
	else if ((rcv.l3bfscHead.ctrlId == HCU_NBIOT_CJ188_CTRL_WRITE_ADDR) && (rcv.l3bfscHead.d0d1Id == HCU_NBIOT_CJ188_WRITE_DI0DI1_ADDRESS)){
		//设置假数据，为了测试方便
		//Do nothing
	}
	else if ((rcv.l3bfscHead.ctrlId == HCU_NBIOT_CJ188_CTRL_WRITE_DEVICE_SYN) && (rcv.l3bfscHead.d0d1Id == HCU_NBIOT_CJ188_WRITE_DI0DI1_DEVICE_SYN_DATA)){
		//设置假数据，为了测试方便
		memset(tmp, 0, sizeof(tmp));
		sprintf(tmp, "%04d", rand()%10000);
		strncpy(l3bfscResp.l3bfscData.st, tmp, 4);
	}
	else{
		HcuErrorPrint("L3BFSC: Receive message error!\n");
		zHcuRunErrCnt[TASK_ID_L3BFSC]++;
		return FAILURE;
	}

	//准备存入本地数据库, RECORD还要存入数据库
	if (HCU_DB_SENSOR_SAVE_FLAG == HCU_DB_SENSOR_SAVE_FLAG_YES)
	{
		sensor_l3bfsc_cj188_data_element_t l3bfscDbSave;
		memset(&l3bfscDbSave, 0, sizeof(sensor_l3bfsc_cj188_data_element_t));
		memcpy(&l3bfscDbSave.l3bfsc, &l3bfscResp.l3bfscData, sizeof(sensor_general_cj188_data_element_t));
		strcpy(l3bfscDbSave.cj188address, l3bfscResp.l3bfscHead.addr);
		l3bfscDbSave.equtype = l3bfscResp.equtype;
		l3bfscDbSave.timestamp = l3bfscResp.l3bfscHead.timestamp;
		l3bfscDbSave.billtodayaccuvolume = l3bfscResp.billtodayaccuvolume;
		l3bfscDbSave.billtodayaccuvolumeunit = l3bfscResp.billtodayaccuvolumeunit;
		ret = dbi_HcuL3bfscCj188DataInfo_save(&l3bfscDbSave);
		if (ret == FAILURE){
			zHcuRunErrCnt[TASK_ID_L3BFSC]++;
			HcuErrorPrint("L3BFSC: Can not save data into database!\n");
		}
	}

	//准备发送数据回去给NBIOTCJ188，然后通过UL上行链路送回CLOUD平台
	l3bfscResp.length = sizeof(msg_struct_l3bfsc_nbiotcj188_data_resp_t);
	ret = hcu_message_send(MSG_ID_L3BFSC_NBIOTCJ188_DATA_RESP, TASK_ID_NBIOTCJ188, TASK_ID_L3BFSC, &l3bfscResp, l3bfscResp.length);
	if (ret == FAILURE){
		zHcuRunErrCnt[TASK_ID_L3BFSC]++;
		HcuErrorPrint("L3BFSC: Send message error, TASK [%s] to TASK[%s]!\n", zHcuTaskNameList[TASK_ID_L3BFSC], zHcuTaskNameList[TASK_ID_NBIOTCJ188]);
		return FAILURE;
	}

	//返回
	return SUCCESS;
}

//暂时不用，留待未来使用
OPSTAT fsm_l3bfsc_nbiotcj188_control_cmd(UINT32 dest_id, UINT32 src_id, void * param_ptr, UINT32 param_len)
{
	return SUCCESS;
}








