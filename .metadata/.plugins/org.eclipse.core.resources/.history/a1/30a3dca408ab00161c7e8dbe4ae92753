	}
	else if ((rcv.l3bfscHead.ctrlId == HCU_NBIOT_CJ188_CTRL_WRITE_DATA) && (rcv.l3bfscHead.d0d1Id == HCU_NBIOT_CJ188_WRITE_DI0DI1_PRICE_TABLE)){
